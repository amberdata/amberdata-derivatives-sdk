{
  "type": "object",
  "properties": {
    "description": { "type": "string" },
    "payload": {
      "type": "object",
      "properties": {
        "data": {
          "type": "array",
          "items": {
            "type": "object",
            "properties": {
              "currency"          : { "type": "string"  },
              "dte"               : { "type": "number"  },
              "expiry"            : { "type": "string"  },
              "forwardDifference" : { "type": "number"  },
              "indexPrice"        : { "type": "number"  },
              "sviA"              : { "type": "number"  },
              "sviB"              : { "type": "number"  },
              "sviM"              : { "type": "number"  },
              "sviRho"            : { "type": "number"  },
              "sviSigma"          : { "type": "number"  },
              "timestamp"         : { "type": ["integer", "string"] }
            },
            "required": ["currency", "dte", "expiry", "forwardDifference", "indexPrice", "sviA", "sviB", "sviM", "sviRho", "sviSigma", "timestamp"],
            "additionalProperties": false
          }
        },
        "metadata": {
          "type": "object",
          "properties": {
            "api-version" : { "type": "string" },
            "signature"   : { "type": "string" }
          },
          "required": ["api-version"],
          "additionalProperties": false
